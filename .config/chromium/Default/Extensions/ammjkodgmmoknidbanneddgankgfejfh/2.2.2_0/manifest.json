{
   "background": {
      "scripts": [ "background.js" ]
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "content.js" ],
      "matches": [ "*://*.twitch.tv/*" ]
   } ],
   "description": "7TV is an emote service and extension for Twitch and YouTube",
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "48": "icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "7TV",
   "optional_permissions": [ "*://*.youtube.com/*" ],
   "page_action": {
      "default_icon": "icon128.png"
   },
   "permissions": [ "*://*.twitch.tv/*", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2.2.2",
   "web_accessible_resources": [ "twitch.js", "youtube.js", "ffz_hook.js", "ffz_addon.js", "styles/Style.css", "image/*.webp" ]
}
